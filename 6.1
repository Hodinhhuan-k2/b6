class Circle(object):
    def __init__(self, r):
        self.radius = r
    
    def area(self):
        return self.radius**2 * 3.14

# Create an instance of the Circle class with radius 2
aCircle = Circle(2)

# Print the area of the circle
print(aCircle.area())
