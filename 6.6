class StringManipulator:
    def __init__(self):
        self.input_string = ""

    def get_String(self):
        # Nhận chuỗi từ người dùng
        self.input_string = input("Nhập một chuỗi: ")

    def print_String(self):
        # In chuỗi dưới dạng chữ in hoa
        print(self.input_string.upper())

# Tạo đối tượng của lớp StringManipulator
string_manipulator = StringManipulator()

# Gọi phương thức get_String để nhận chuỗi từ người dùng
string_manipulator.get_String()

# Gọi phương thức print_String để in chuỗi in hoa
string_manipulator.print_String()
